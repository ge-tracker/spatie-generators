{
    "name": "ge-tracker/spatie-generators",
    "description": "Artisan make generators for Spatie Actions and DTO",
    "keywords": [
        "laravel",
        "spatie",
        "generator",
        "action",
        "dto"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/ge-tracker/spatie-generators",
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-queueable-action": "^2.5",
        "spatie/data-transfer-object": "^2.0",
        "illuminate/console": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "GeTracker\\SpatieGenerators\\": "src"
        }
    },
    "authors": [
        {
            "name": "GE Tracker",
            "email": "support@ge-tracker.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeTracker\\SpatieGenerators\\SpatieGeneratorServiceProvider"
            ]
        }
    }
}
